[core]
	excludesfile = ~/devtools/install/devtools/dotfiles/git/gitignore-global
	editor = vim
[alias]
	br = branch
	ci = commit
	co = checkout
	cp = cherry-pick
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	ll = log --graph
	ls = log --graph --oneline --no-decorate
	rb = rebase
	rbi = rebase -i
	release-notes = log --oneline --no-decorate --no-merges
	rst = restore
	sb = submodule
	st = status
